{
  "expo": {
    "name": "vanlife",
    "slug": "vanlife",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-blue.png",
    "scheme": "vanlife",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-blue.png",
        "backgroundColor": "#00BFFF"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-blue.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-white.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#00BFFF"
        }
      ],
      "expo-font"
    ],

    "experiments": {
      "typedRoutes": true
    },

    "assetBundlePatterns": ["**/*"],

    "extra": {
      "EXPO_PUBLIC_FIREBASE_API_KEY": "AIzaSyCcWRhZ1whcdv_zgN3iFlxTKOHy-QoV8wM",
      "EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN": "wandermeet-app.firebaseapp.com",
      "EXPO_PUBLIC_FIREBASE_PROJECT_ID": "wandermeet-app",
      "EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET": "wandermeet-app.appspot.com",
      "EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": "146224476711",
      "EXPO_PUBLIC_FIREBASE_APP_ID": "1:146224476711:web:a7c3e2792534592862d402"
    }
  }
}
